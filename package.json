{
  "name": "react-hook-useagent",
  "version": "1.2.2",
  "description": "A react hook which provides a simple way to get the user agent of the browser",
  "author": {
    "name": "Flo Ragossnig",
    "email": "florian.ragossnig@me.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Poolshark/react-hook-useagent.git"
  },
  "module": "index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "keywords": [
    "react",
    "react-hook",
    "user-agent",
    "browser",
    "device",
    "os",
    "mobile"
  ],
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^24.10.2",
    "bun-types": "^1.3.4",
    "fast-check": "^4.4.0",
    "jsdom": "^27.3.0",
    "rimraf": "^6.1.2",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "typescript": "^5.7.3",
    "react": "^19.0.0"
  },
  "dependencies": {
    "@types/react": "^19.2.7",
    "latest": "^0.2.0"
  },
  "scripts": {
    "build": "bun build --target=node ./src/index.ts --outfile=dist/index.js --minify --external react && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "postbuild": "rimraf tsconfig.types.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "files": ["dist/*.js", "dist/*.d.ts"]
}
